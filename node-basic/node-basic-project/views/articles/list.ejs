<!--우측 상단 헤더 영역 -->
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>관리자정보관리</h2>
        <ol class="breadcrumb">
            <li>
                <a href="/Home/Index">Home</a>
            </li>
            <li>
                <a>관리자정보관리</a>
            </li>
            <li class="active">
                <strong>조회</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">

    </div>
</div>

<!--우측 콘텐츠 영역 -->
<div class="wrapper wrapper-content">
    <!--검색 영역 -->
    <div class="ibox-content m-b-sm border-bottom">
        <div class="row">
            <div class="col-sm-4">
                <div class="form-group">
                    <label class="control-label" for="OrderNo">관리자명</label>
                    <input type="text" class="form-control">
                </div>
            </div>
            <div class="col-sm-4">
                <div class="form-group">
                    <label class="control-label" for="OrderName">아이디</label>
                    <input type="text" class="form-control">
                </div>
            </div>
            <div class="col-sm-4">
                <div class="form-group">
                    <label class="control-label" for="MobileNo">사용여부</label>
                    <input type="text" class="form-control">
                </div>
            </div>
        </div>
        <div class="text-center">
            <button type="button" class="btn btn-primary">조회</button>
            <a href="/articles/create" class="btn btn-info">신규</a>
        </div>
    </div>

    <!--목록 영역 -->
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-content">
                    <div class="table-responsive">

                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th width="10%">번호</th>
                                    <th width="40%">제목</th>
                                    <th width="10%">조회수</th>
                                    <th width="10%">IP</th>
                                    <th width="10%">게시여부</th>
                                    <th width="10%">작성자</th>
                                    <th width="10%">작성일</th>
                                </tr>
                            </thead>
                            <tbody class="hoverTblBody">
                                <% for(var i=0;i < articleList.length;i++){ %>
                                    <tr>
                                        <td><a href="/articles/update?idx=<%=articleList[i].idx%>">
                                                <%=articleList[i].idx%>
                                            </a></td>
                                        <td><a href="/articles/update/<%=articleList[i].idx%>">
                                                <%=articleList[i].title%>
                                            </a></td>
                                        <td>
                                            <%=articleList[i].viewCnt%>
                                        </td>
                                        <td>
                                            <%=articleList[i].ip%>
                                        </td>
                                        <td>
                                            <%=articleList[i].displayYn%>
                                        </td>
                                        <td>
                                            <%=articleList[i].register%>
                                        </td>
                                        <td>
                                            <%=articleList[i].registDate%>
                                        </td>
                                    </tr>
                                    <% } %>
                            </tbody>
                        </table>

                    </div>
                </div>
            </div>
        </div>

    </div>


</div>

<script>
    console.log("게시글 목록 페이지 로딩 완료");
</script>